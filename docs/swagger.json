          {
            "name": "phoneNumber",
            "in": "formData",
            "description": "Telephone Number",
            "required": false,
            "type": "string",
            "default":"1234569874"
          },
          {
            "name": "gender",
            "in": "formData",
            "description": "Gender male/female/others",
            "required": true,
            "type": "string",
            "default":"male"
          },
          {
            "name":"countryCode",
            "in":"formData",
            "description":"Mobile Code",
            "required":false,
            "type": "string",
            "default":"+91"
          },
          {
            "name":"role",
            "in":"formData",
            "description":"role",
            "required":false,
            "type": "string",
            "default":"1"
            
          },
          {
            "name":"stateId",
            "in":"formData",
            "description":"stateId",
            "required":false,
            "type": "string",
            "default":"1"
          },
          {
            "name":"countryId",
            "in":"formData",
            "description":"countryId",
            "required":true,
            "type": "string",
            "default":"1"
          },
          {
